# if imported, path gets excluded from authentication
(common_noauth) {
    root * /app/public
    encode zstd gzip
    file_server
}

:80

log

route {
    # Basic Authentication for other paths
    handle {
        basicauth /* {
            #BASICAUTH_USER BASICAUTH_PASSWD#
        }

        root * /app/public
        encode zstd gzip
        file_server
    }
}

